{
  "name": "rbush-ext",
  "version": "2.0.3",
  "description": "Fork of @mourner's rBush package, allows for meaningful use of the internal tree groupings/nodes.",
  "homepage": "https://github.com/sikanrong/rbush",
  "repository": {
    "type": "git",
    "url": "git://github.com/sikanrong/rbush.git"
  },
  "keywords": [
    "spatial",
    "tree",
    "search",
    "rectangle",
    "index",
    "math"
  ],
  "author": "Vladimir Agafonkin, Alexander Pilafian",
  "license": "MIT",
  "main": "index.js",
  "devDependencies": {
    "benchmark": "^2.1.0",
    "browserify": "^13.0.1",
    "eslint": "^2.10.2",
    "eslint-config-mourner": "^2.0.1",
    "faucet": "0.0.1",
    "istanbul": "~0.4.3",
    "tape": "^4.5.1",
    "uglify-js": "^2.6.4",

    "three": "0.81.2",
    "lodash": "latest",
    "three-map-controls": "latest"
  },
  "scripts": {
    "test": "eslint index.js test/test.js && node test/test.js | faucet",
    "perf": "node ./bench/perf.js",
    "cov": "istanbul cover test/test.js -x test/test.js",
    "build": "browserify index.js -s rbush -o rbush.js",
    "build-min": "browserify index.js -s rbush | uglifyjs -c warnings=false -m > rbush.min.js",
    "prepublish": "npm run build && npm run build-min"
  },
  "eslintConfig": {
    "extends": "mourner",
    "rules": {
      "new-cap": 0,
      "consistent-return": 0
    }
  },
  "dependencies": {
    "quickselect": "^1.0.0",
    "uuid": "2.0.3"
  }
}
